        .link [0] a ( .go-to-last-post )( .lastpostdate ) | "%(href)v"
    },
    .lastpost_link [0] a .lastpostdate | "%(href)v",
    .lastpost2 * c@[3:] title=aE>"[0-9]+.*,.*[0-9]+"; div .smallfont child@; {
