            },
            .date {
                [0] * ( .lastpost-date )( .lastpostdate )( .time ) | "%DT" trim,
                [-] div; * self@ c@[:5] | "%DT" trim
            },
            .user {
                * ( .lastpost-by )( .lastpostby ); [0] a ||
                * .title ||
                [-1] div; * c@[:1] self@
            }; [0] * c@[0] m@>[1:] | "%Di" trim,
            .user_link {
                * ( .lastpost-by )( .lastpostby ) ||
                [-1] div; * c@[1] self@
            }; [0] a; * self@ | "%(href)v",
            .avatar a .avatar; [0] img | "%(src)v"
        }
    } |
} | ,

.threads {
