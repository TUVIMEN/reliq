        }; {
            .title [0] * c@[0] m@>[1:] | "%Di" trim,
            .link [0] a | "%(href)v"
        },
        .description {
            [0] * ( .forum-desc )( .forumdescription )( .description ) | "%i" ||
            div .smallfont; {
                [0] * c@[0] m@b>[1:] self@ | "%i" ||
